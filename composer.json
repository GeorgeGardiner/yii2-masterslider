{
    "name": "commonmode/yii2-masterslider",
    "description": "Yii 2 extension for jQuery Master Slider. You will need to purchase a license for Master Slider before you can use this package.",
    "type": "yii2-extension",
    "license": "BSD",
    "authors": [
        {
            "name": "George Gardiner",
            "email": "george.gardiner@commonmode.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "bower-asset/masterslider": "2.15.1"
    },
    "autoload": {
        "psr-4": {
            "commonmode\\yii2-masterslider\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },    
        "asset-repositories": [
            {
                "type": "bower-vcs",
                "url": "gitlab@gitlab.commonmode.co.uk:web/masterslider.git",
                "name": "bower-asset/masterslider"
            }
        ]    
    }
}
